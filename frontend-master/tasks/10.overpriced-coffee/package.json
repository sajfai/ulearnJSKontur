{
  "name": "10.overpriced-coffee",
  "scripts": {
    "start": "better-npm-run start",
    "debug": "better-npm-run debug"
  },
  "betterScripts": {
    "debug": {
      "command": "node --experimental-modules index.mjs",
      "env": {
        "DEBUG": "express:*"
      }
    },
    "start": {
      "command": "nodemon --experimental-modules index.mjs",
      "env": {
        "DEBUG": "express:*"
      }
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "nodemon": "^1.19.4"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "better-npm-run": "0.1.1"
  }
}
